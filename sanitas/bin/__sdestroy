# We unset all our environment variables
if test "x$S_EXPORTED" = "x1"; then 
  # function defined in __sexport
  srestore 
  unset S_DYLD_LIBRARY_PATH
  unset S_LD_LIBRARY_PATH
  unset S_C_INCLUDE_PATH
  unset S_ACLOCAL_PATH
  unset S_PKG_CONFIG_PATH
  unset S_MANPATH
  unset S_GTK_MODULE_DIR
  unset S_libexecdir
  unset S_pythondir
  unset S_PYTHONPATH
  unset S_PATH
  unset S_EXPORTED

  unset S_NOTDETECTED
  unset PROMPT_COMMAND
fi
